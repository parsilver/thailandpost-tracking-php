{
    "name": "farzai/thailand-post",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "PA",
            "role": "DevOps"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "farzai/transport": "^1.2.0",
        "nyholm/psr7": "^1.4",
        "nyholm/psr7-server": "^1.0",
        "symfony/cache": "^7.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.15",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Farzai\\ThaiPost\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Farzai\\Tests\\":  "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
